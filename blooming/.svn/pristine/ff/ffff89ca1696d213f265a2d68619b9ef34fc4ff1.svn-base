	public void setSalHealth(int salHealth) {
		this.salHealth = salHealth;
	}

	public int getSalHire() {
		return salHire;
	}

	public void setSalHire(int salHire) {
		this.salHire = salHire;
	}

	public int getSalIncome() {
		return salIncome;
	}

	public void setSalIncome(int salIncome) {
		this.salIncome = salIncome;
	}

	public int getSalLocal() {
		return salLocal;
	}

	public void setSalLocal(int salLocal) {
		this.salLocal = salLocal;
	}

	public int getSalMsum() {
		return salMsum;
	}

	public void setSalMsum(int salMsum) {
		this.salMsum = salMsum;
	}

	public int getSalPay() {
		return salPay;
	}

	public void setSalPay(int salPay) {
		this.salPay = salPay;
	}

	public Employee getEmployee() {
		return employee;
	}

	public void setEmployee(Employee employee) {
		this.employee = employee;
	}
	
	
	
	



