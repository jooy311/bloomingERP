package com.itwill.dto;

public class Salary {
	private String salNo;
	private String salEmpNo;
	private String salMonth;
	private int salBase;
	private int salGrade;
	private int salHoliday;
	private int salPsum;
	private int salAnnuity;
	private int salHealth;
	private int salHire;
	private int salIncome;
	private int salLocal;
	private int salMsum;
	private int salPay;
	
	private Employee employee;
	
	public Salary() {
		// TODO Auto-generated constructor stub
	}

	public String getSalNo() {
		return salNo;
	}

	public void setSalNo(String salNo) {
		this.salNo = salNo;
	}

	public String getSalEmpNo() {
		return salEmpNo;
	}

	public void setSalEmpNo(String salEmpNo) {
		this.salEmpNo = salEmpNo;
	}

	public String getSalMonth() {
		return salMonth;
	}

	public void setSalMonth(String salMonth) {
		this.salMonth = salMonth;
	}

	public int getSalBase() {
		return salBase;
	}

	public void setSalBase(int salBase) {
		this.salBase = salBase;
	}

	public int getSalGrade() {
		return salGrade;
	}

	public void setSalGrade(int salGrade) {
		this.salGrade = salGrade;
	}

	public int getSalHoliday() {
		return salHoliday;
	}

	public void setSalHoliday(int salHoliday) {
		this.salHoliday = salHoliday;
	}

	public int getSalPsum() {
		return salPsum;
	}

	public void setSalPsum(int salPsum) {
		this.salPsum = salPsum;
	}

