<%@ page language="java" contentType="text/html; charset=UTF-8"
    pageEncoding="UTF-8"%>
<script src="https://ajax.googleapis.com/ajax/libs/jquery/1.12.4/jquery.min.js"></script>

<div class="container">
<div class="block-header">

<h2>발주 정보 변경</h2>
<hr>

<div class="card">
    <div class="card-header">
    
		<div class="tab-content">
				
			<div class="container1">
				<form action="poUpdate" method="post">
				
					<input type="hidden" name="poNo" value="${poSelect.poNo}">
					
					<div class="row">
						<div class="col-25">
							<label for="po_order_no" class="label-pre">주문번호</label>
						</div>
	
						<div class="col-75">
							<div class="fg-line form-group">
								<input type="text" class="form-control input-sm" name="poOrderNo" value="${poSelect.poOrderNo}" readonly>
							</div>
						</div>
					</div>
				
					<div class="row">
					
						<div class="col-25">
							<label for="po_emp_no" class="label-pre">담당사원번호</label>
						</div>
	
						<div class="col-75">
							<div class="fg-line form-group">
								<input type="text" class="form-control input-sm" name="poEmpNo" value="${poSelect.poEmpNo}" readonly>
							</div>
						</div>
					</div>
				
					<div class="row">
					
						<div class="col-25">
							<label for="po_clt_no" class="label-pre">거래처코드</label>
						</div>
	
						<div class="col-75">
							<div class="fg-line form-group">
								<input type="text" class="form-control input-sm" name="poCltNo" value="${poSelect.poCltNo}" readonly>
							</div>
						</div>
					</div>
				
					<div class="row">
					
						<div class="col-25">
							<label for="poProduct" class="label-pre">발주상품</label>
						</div>
	
						<div class="col-75">
							<div class="fg-line form-group">
								<input type="text" class="form-control input-sm" name="poProduct" value="${poSelect.poProduct}" readonly>
							</div>
						</div>
					</div>
					
					<div class="row">
					
						<div class="col-25">
							<label for="po_cnt" class="label-pre">발주수량</label>
						</div>
	
						<div class="col-75">
							<div class="fg-line form-group">
								<input type="text" class="form-control input-sm" name="poCnt" value="${poSelect.poCnt}" readonly>
							</div>
						</div>
					</div>
					
					<div class="row">
					
						<div class="col-25">
							<label for="po_date" class="label-pre">발주날짜</label>
						</div>
	
						<div class="col-75">
							<div class="fg-line form-group">
								<input type="text" class="form-control input-sm" name="poDate" value="${poSelect.poDate}" readonly>
							</div>
						</div>
					</div>
					
					<div class="row">
					
						<div class="col-25">
							<label for="po_price" class="label-pre">발주단가</label>
						</div>
	
						<div class="col-75">
							<div class="fg-line form-group">
								<input type="text" class="form-control input-sm" name="poPrice" value="${poSelect.poPrice}">
							</div>
						</div>
					</div>
			
				<button type="submit" class="btn btn-primary waves-effect waves-button waves-float" style="float : right">수정</button>
				</form>
			</div>
		</div>
	</div>
</div>
</div>
</div>


<script type="text/javascript">
	

</script>