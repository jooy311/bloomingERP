package com.itwill.dto;

public class Approval {
	private String apprNo;
	private String apprAuthNo;
	private String apprEmpNo;
	private String apprDate;
	
	private Authorization authorization;
	private Employee employee;
	
	public Approval() {
		// TODO Auto-generated constructor stub
	}
	
	public String getApprNo() {
		return apprNo;
	}
	public void setApprNo(String apprNo) {
		this.apprNo = apprNo;
	}
	public String getApprAuthNo() {
		return apprAuthNo;
	}
	public void setApprAuthNo(String apprAuthNo) {
		this.apprAuthNo = apprAuthNo;
	}
	public String getApprEmpNo() {
		return apprEmpNo;
	}
	public void setApprEmpNo(String apprEmpNo) {
		this.apprEmpNo = apprEmpNo;
	}
	public String getApprDate() {
		return apprDate;
	}
	public void setApprDate(String apprDate) {
		this.apprDate = apprDate;
	}
	public Authorization getAuthorization() {
		return authorization;
	}
	public void setAuthorization(Authorization authorization) {
		this.authorization = authorization;
	}
	public Employee getEmployee() {
		return employee;
	}
	public void setEmployee(Employee employee) {
		this.employee = employee;
	}
	
	
}
