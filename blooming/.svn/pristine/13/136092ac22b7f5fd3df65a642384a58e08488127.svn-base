package com.itwill.dto;

public class Attitude {
	private String attdNo;
	private String attdEmpNo;
	private String attdContent;
	private String attdCase;
	private String attdStime;
	private String attdEtime;
	private String attdDate;
	private String attdNoCount;
	
	private Employee employee;
		
	public Attitude() {
		// TODO Auto-generated constructor stub
	}

	public String getAttdNo() {
		return attdNo;
	}

	public void setAttdNo(String attdNo) {
		this.attdNo = attdNo;
	}

	public String getAttdEmpNo() {
		return attdEmpNo;
	}

	public void setAttdEmpNo(String attdEmpNo) {
		this.attdEmpNo = attdEmpNo;
	}

	public String getAttdContent() {
		return attdContent;
	}

	public void setAttdContent(String attdContent) {
		this.attdContent = attdContent;
	}

	public String getAttdCase() {
		return attdCase;
	}

	public void setAttdCase(String attdCase) {
		this.attdCase = attdCase;
	}	
	
	public String getAttdStime() {
		return attdStime;
	}

	public void setAttdStime(String attdStime) {
		this.attdStime = attdStime;
	}

	public String getAttdEtime() {
		return attdEtime;
	}

	public void setAttdEtime(String attdEtime) {
		this.attdEtime = attdEtime;
	}

	public String getAttdDate() {
		return attdDate;
	}

	public void setAttdDate(String attdDate) {
		this.attdDate = attdDate;
	}

	public Employee getEmployee() {
		return employee;
	}

	public void setEmployee(Employee employee) {
		this.employee = employee;
	}

	public String getAttdNoCount() {
		return attdNoCount;
	}

	public void setAttdNoCount(String attdNoCount) {
		this.attdNoCount = attdNoCount;
	}
	
	
	
	

}
